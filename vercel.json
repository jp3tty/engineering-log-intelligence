{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    },
    {
      "src": "login.html",
      "use": "@vercel/static"
    },
    {
      "src": "dashboard.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/health_public.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/health.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/logs.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/auth.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/dashboard_analytics.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/incident_response.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/ml/analyze.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/ml/real_time.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/ml/ab_testing.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/simple-dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}