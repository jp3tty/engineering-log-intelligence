{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node"
    },
    "api/**/*.py": {
      "runtime": "@vercel/python"
    }
  }
}